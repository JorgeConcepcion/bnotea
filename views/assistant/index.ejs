<%include ../partials/header%>



<!--Displaying all the analysts-->
<div class="container-fluid" style="padding-top: 60px;">
    <!--ROW START-->
    <div class="row">
        <%assistants.forEach(function(assistant){%>
            <div class="col-md-4 col-sm-6 col-xs-12" >
                <div class="text-center">
                    <a  href="/superuser/<%=superuserID%>/assistant/<%=assistant._id%>"><img src="<%=assistant.photo%>" class="avatar img-circle img-thumbnail index-image" alt="avatar"></a>
                    <div class="caption">
                        <h3><%=assistant.firstName+" "+assistant.lastName%></h3>
                        <p><strong>Phone: </strong><%=assistant.phone%></p>
                        <p><strong>Email: </strong><%=assistant.email%></p>
                        <p><strong>Provider ID: </strong><%=assistant.providerId%></p>
                    </div>
                </div>
            </div>
        <%})%>
    </div>
    <!--ROW FINISH-->
</div>



<%include ../partials/footer%>