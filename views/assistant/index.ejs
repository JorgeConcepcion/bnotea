                    <div class="row">
                        <%superuser.assistants.forEach(function(assistant){%>
                        <div class="col-md-4 col-sm-6 col-xs-12">
                            <div class="thumbnail">
                                <div class="text-center">
                                     <a id="image" href="/superuser/<%=superuser._id%>/assistant/<%=assistant._id%>"><img src="<%=assistant.photo%>" class="avatar img-circle img-thumbnail" alt="avatar"></a>
