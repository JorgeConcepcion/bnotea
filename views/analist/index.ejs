<%include ../partials/header%>



<!--Displaying all the analists-->
<div class="container-fluid" style="padding-top: 60px;">
    <!--ROW START-->
    <div class="row">
        <%analists.forEach(function(analist){%>
            <div class="col-md-4 col-sm-6 col-xs-12">
                <div class="text-center">
                   <a href="/superuser/<%=superuserID%>/analist/<%=analist._id%>"><img src="<%=analist.photo%>" class="avatar img-circle img-thumbnail index-image" alt="avatar"></a>
                    <div class="caption">
                        <h3><%=analist.firstName+" "+analist.lastName%></h3>
                        <p><strong>Phone: </strong><%=analist.phone%></p>
                        <p><strong>Email: </strong><%=analist.email%></p>
                        <p><strong>Provider ID: </strong><%=analist.providerId%></p>
                    </div>
                </div>
            </div>
        <%})%>
    </div>
    <!--ROW FINISH-->
</div>
    


<%include ../partials/footer%>